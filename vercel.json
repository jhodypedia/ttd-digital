{
  "version": 2,
  "builds": [
    { "src": "/*.html", "use": "@vercel/static" },
    { "src": "/style.css", "use": "@vercel/static" },
    { "src": "/main.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/login", "dest": "/login.html" },
    { "src": "/register", "dest": "/register.html" },
    { "src": "/deposit", "dest": "/deposit.html" },
    { "src": "/riwayat", "dest": "/riwayat.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
